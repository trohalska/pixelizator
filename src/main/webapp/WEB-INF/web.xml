<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>SimpleServlet</servlet-name>
        <servlet-class>world.ucode.SimpleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StatusCodeDemo</servlet-name>
        <servlet-class>world.ucode.StatusCodeDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pixelizator</servlet-name>
        <servlet-class>world.ucode.Pixelizator</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>SimpleServlet</servlet-name>
        <url-pattern>/SimpleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StatusCodeDemo</servlet-name>
        <url-pattern>/StatusCodeDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pixelizator</servlet-name>
        <url-pattern>/Pixelizator</url-pattern>
    </servlet-mapping>

</web-app>